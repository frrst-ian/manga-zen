<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="styles.css" />
    <title>Manga Zen - Genre</title>
  </head>
  <body>
    <header>
      <div class="container">
        <div class="logo"></div>
        <nav>
          <ul>
            <li>
              <a href="/"> Home </a>
              <a href="/manga">Manga</a>
              <a href="/genres">Genres</a>
            </li>
          </ul>
        </nav>
      </div>
    </header>
    <h1>Manga Genres</h1>
    <ul>
      <div class="genre-list">
        <% genres.forEach(genre => { %>
        <div class="genres-item">
          <li>
            <a href="genres/genre-id/<%=genre.genre_id%>"><%=genre.genre_name%></a>
          </li>
        </div>
        <%}) %>
      </div>
    </ul>
  </body>
</html>
